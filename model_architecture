digraph Model_Architecture {
	bgcolor=transparent color="#DAFFFB" fontcolor="#DAFFFB" rankdir=TB
	Input_CNN [label="Input CNN (10x10x1)" color="#DAFFFB" fontcolor="#DAFFFB"]
	Conv2D [label="Conv2D (16 filtros)" color="#DAFFFB" fontcolor="#DAFFFB"]
	MaxPooling [label="MaxPooling (5x5x16)" color="#DAFFFB" fontcolor="#DAFFFB"]
	Flatten_CNN [label="Flatten (400)" color="#DAFFFB" fontcolor="#DAFFFB"]
	Input_LSTM [label="Input LSTM (24x5)" color="#DAFFFB" fontcolor="#DAFFFB"]
	LSTM [label="LSTM (32 unidades)" color="#DAFFFB" fontcolor="#DAFFFB"]
	BatchNorm_LSTM [label="Batch Normalization (LSTM)" color="#DAFFFB" fontcolor="#DAFFFB"]
	Input_Category [label="Input Category (1)" color="#DAFFFB" fontcolor="#DAFFFB"]
	Embedding [label="Embedding (8 dim)" color="#DAFFFB" fontcolor="#DAFFFB"]
	Flatten_Embedding [label="Flatten (8)" color="#DAFFFB" fontcolor="#DAFFFB"]
	Concatenation [label=Concatenation color="#DAFFFB" fontcolor="#DAFFFB"]
	Dense [label="Dense Layer (32 unidades)" color="#DAFFFB" fontcolor="#DAFFFB"]
	Dropout [label="Dropout (30%)" color="#DAFFFB" fontcolor="#DAFFFB"]
	Output [label="Output Layer (5 clases)" color="#DAFFFB" fontcolor="#DAFFFB"]
	Input_CNN -> Conv2D [color="#DAFFFB"]
	Conv2D -> MaxPooling [color="#DAFFFB"]
	MaxPooling -> Flatten_CNN [color="#DAFFFB"]
	Input_LSTM -> LSTM [color="#DAFFFB"]
	LSTM -> BatchNorm_LSTM [color="#DAFFFB"]
	Input_Category -> Embedding [color="#DAFFFB"]
	Embedding -> Flatten_Embedding [color="#DAFFFB"]
	Flatten_CNN -> Concatenation [color="#DAFFFB"]
	BatchNorm_LSTM -> Concatenation [color="#DAFFFB"]
	Flatten_Embedding -> Concatenation [color="#DAFFFB"]
	Concatenation -> Dense [color="#DAFFFB"]
	Dense -> Dropout [color="#DAFFFB"]
	Dropout -> Output [color="#DAFFFB"]
}
